{
  "name": "lofiever",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node -P tsconfig-seed.json prisma/seed.ts"
  },
  "scripts": {
    "dev": "ts-node -r dotenv/config -P tsconfig-seed.json server/index.ts",
    "dev:server": "ts-node -P tsconfig-seed.json server/index.ts",
    "dev:next": "next dev --turbopack",
    "build": "next build",
    "build:server": "esbuild server/index.ts --bundle --platform=node --target=node20 --outfile=dist/server.js --external:next --external:@prisma/client --external:ioredis --packages=external",
    "build:scripts": "esbuild scripts/seed-database.ts scripts/generate-seed-mock.ts scripts/clear-chat.ts scripts/reset-playlist.ts scripts/diagnose-playlist.ts scripts/import-sql.ts --bundle --platform=node --target=node20 --outdir=dist/scripts --external:@prisma/client --packages=external",
    "start": "node dist/server.js",
    "start:next": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:redis": "ts-node -P tsconfig-seed.json scripts/test-redis.ts",
    "docker:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:restart": "docker-compose -f docker-compose.dev.yml restart",
    "setup": "./scripts/start-app.sh",
    "stream:test": "curl -s http://localhost:8000/status-json.xsl | jq '.'",
    "stream:listen": "echo 'Stream URL: http://localhost:8000/stream'",
    "stream:monitor": "./scripts/monitor-stream.sh",
    "stream:watch": "./scripts/monitor-stream.sh --watch",
    "db:seed": "ts-node -P tsconfig-seed.json scripts/seed-database.ts",
    "db:seed:prod": "node dist/scripts/seed-database.js",
    "db:import": "ts-node -P tsconfig-seed.json scripts/import-sql.ts",
    "db:import:prod": "node dist/scripts/import-sql.js prisma/seeds/production_dump.sql",
    "db:dump-mock": "ts-node -P tsconfig-seed.json scripts/generate-seed-mock.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "chat:clear": "ts-node -P tsconfig-seed.json scripts/clear-chat.ts",
    "session:clear": "ts-node -P tsconfig-seed.json scripts/clear-sessions.ts",
    "listeners:reset": "ts-node -P tsconfig-seed.json scripts/reset-listeners.ts",
    "playlist:reset": "ts-node -P tsconfig-seed.json scripts/reset-playlist.ts",
    "playlist:diagnose": "ts-node -P tsconfig-seed.json scripts/diagnose-playlist.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.69",
    "@ai-sdk/react": "^2.0.98",
    "@aws-sdk/client-s3": "^3.936.0",
    "@aws-sdk/s3-request-presigner": "^3.936.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.6.0",
    "@tanstack/react-query": "^5.74.4",
    "ai": "^5.0.98",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "ioredis": "^5.6.1",
    "isomorphic-dompurify": "^2.35.0",
    "nanoid": "^5.1.5",
    "next": "15.3.0",
    "next-auth": "^4.24.11",
    "next-intl": "^4.5.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.2.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.5",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/postcss": "^4.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/ioredis": "^5.0.0",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/socket.io-client": "^3.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "music-metadata": "^11.10.2",
    "postcss": "^8.5.3",
    "prisma": "^6.6.0",
    "tailwindcss": "^4.1.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
